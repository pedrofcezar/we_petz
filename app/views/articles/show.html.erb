<div class="wrapper">
    <div class="container title">
        <%= @article.title %>
    </div>
    <div style="height: 25px"></div>
    <div class="keys">
        <p> Este artigo refere-se a...</p>
        <p>
            <% if @article.dog_key %>
            <i class="fas fa-dog"></i>
            <% else %> <span></span>
            <% end %>
            <% if @article.cat_key %><i class="fas fa-cat"></i>
            <% else %> <span></span>
            <% end %>
            <% if @article.product_key %><i class="fas fa-box-open"></i>
            <% else %> <span></span>
            <% end %>
            <% if @article.pet_key %><i class="fas fa-paw"></i>
            <% else %> <span></span>
            <% end %>
        </p>
    </div>
    <div style="height: 25px"></div>
    <div class="container content">
        <%= @article.content %>
    </div>
    <div style="height: 25px"></div>
    <%= link_to @article.title, edit_article_path(@article)  %>
     <div style="height: 25px"></div>
    <%= link_to "Voltar", articles_path  %>
</div>
